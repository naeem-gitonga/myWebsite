{
  "name": "jng-personal-website-backend",
  "version": "1.0.0",
  "description": "back-end resources",
  "main": "handler.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watchAll",
    "serve:local": "sls offline start --noPrependStageInUrl --reloadHandler",
    "start": "serverless offline",
    "why:test:dev": "echo \"cause you can't have a watch flag in a CI/CD pipeline... it will block the pipeline\"",
    "coverage": "jest --collect-coverage",
    "package": "SLS_DEBUG=* && sls package",
    "package:production:locally": "SLS_DEBUG=* && echo \"NODE_ENV=production\" >> .env && sls package && sed -i '' '$d' .env",
    "package:staging:locally": "SLS_DEBUG=* && echo \"NODE_ENV=staging\" >> .env && sls package && sed -i '' '$d' .env"
  },
  "repository": {
    "type": "git",
    "url": "\""
  },
